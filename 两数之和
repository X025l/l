public class _02 {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        while (scan.hasNext()) {
            int a = scan.nextInt();
            int b = scan.nextInt();
            int result = add(a, b);
            System.out.println(result);
        }
        scan.close();
    }
    public static int add(int a, int b) {
        int c = a + b;
        return c;


}

}
